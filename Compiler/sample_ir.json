{
  "version": "1.0",
  "metadata": {
    "created_by": "example_user",
    "created_at": "2024-05-01T12:00:00Z",
    "robot_brand": "fanuc",
    "description": "Sample pick-and-place workflow"
  },
  "sequence": [
    {
      "id": "connect",
      "command": "connect_robot",
      "parameters": {
        "ip_address": "192.168.0.10",
        "port": 60008,
        "timeout_ms": 5000
      }
    },
    {
      "id": "power_on",
      "command": "power_on_servo"
    },
    {
      "id": "set_speed",
      "command": "set_speed",
      "parameters": {
        "percentage": 75
      }
    },
    {
      "id": "pickup_pose",
      "command": "move_linear",
      "parameters": {
        "x": 250.0,
        "y": 150.0,
        "z": 120.0,
        "rx": 0.0,
        "ry": 180.0,
        "rz": 90.0,
        "speed_percentage": 40
      },
      "comment": "Move above the part"
    },
    {
      "id": "grip",
      "command": "grab"
    },
    {
      "id": "place_pose",
      "command": "move_linear",
      "parameters": {
        "x": 360.0,
        "y": -120.0,
        "z": 80.0,
        "rx": 0.0,
        "ry": 180.0,
        "rz": 0.0
      }
    },
    {
      "id": "release",
      "command": "release"
    },
    {
      "id": "stop",
      "command": "stop_motion",
      "parameters": {
        "stop_type": "normal"
      }
    },
    {
      "id": "disconnect",
      "command": "disconnect_robot"
    }
  ]
}
